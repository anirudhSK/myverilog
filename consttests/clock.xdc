set_property IOSTANDARD  LVCMOS33 [get_ports {o_rand_output[2]}]
set_property PACKAGE_PIN W18      [get_ports {o_rand_output[3]}]
set_property IOSTANDARD  LVCMOS33 [get_ports {o_rand_output[3]}]
set_property PACKAGE_PIN V19      [get_ports {o_rand_output[4]}]
set_property IOSTANDARD  LVCMOS33 [get_ports {o_rand_output[4]}]
set_property PACKAGE_PIN U19      [get_ports {o_rand_output[5]}]
set_property IOSTANDARD  LVCMOS33 [get_ports {o_rand_output[5]}]
set_property PACKAGE_PIN E19      [get_ports {o_rand_output[6]}]
