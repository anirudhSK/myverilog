# W5 is the oscillator pin
set_property PACKAGE_PIN W5 [get_ports clk]
set_property IOSTANDARD LVCMOS33 [get_ports clk]

# create clock
create_clock -period  3 -name clock_object [get_ports clk]

# outputs
set_property PACKAGE_PIN V14      [get_ports {o_rand_output[0]}]
set_property IOSTANDARD  LVCMOS33 [get_ports {o_rand_output[0]}]
